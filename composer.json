{
    "name": "php-etl/sql-flow",
    "description": "SQL implementation for the ETL pipeline",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "php-etl/pipeline-contracts": "^0.2.0",
        "psr/log": "^1.1@dev",
        "php-etl/bucket": "^0.2.0",
        "php-etl/mapping-contracts": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "php-etl/phpunit-extension": "^0.2.0",
        "phpstan/phpstan": "^0.12.92"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Flow\\SQL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "unit\\Kiboko\\Component\\Flow\\SQL\\": "tests/unit/",
            "functional\\Kiboko\\Component\\Flow\\SQL\\": "tests/functional/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
